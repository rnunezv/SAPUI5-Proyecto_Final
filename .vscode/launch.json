{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Gestion-Empleados",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/gestion-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/logaligroupEmpleados/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/Empleados",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Gestion/Empleados\"}"
      },
      "envFile": "${workspaceFolder}/Empleados/.env1",
      "preLaunchTask": "Build Gestion"
    }
  ]
}